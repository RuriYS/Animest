{
    "compilerOptions": {
        "baseUrl": "./resources",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "es5",
        "paths": {
            "@/*": [ "./src/*" ],
            "@/types/*": [ "./types/* "],
        }
    },
    "include": [
        "resources/src",
        "resources/types"
    ]
}
