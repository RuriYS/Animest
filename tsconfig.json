{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "es5",
        "module": "esnext",
        "strict": true,
        "jsx": "react",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
    },
    "include": [ "resources/src/**/*" ]
}
